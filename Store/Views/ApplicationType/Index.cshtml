@model IEnumerable<ApplicationType>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Application Type List</h2>
        </div>
        <div class="col-6">
            <a asp-controller="ApplicationType" asp-action="Create" class="btn btn-secondary">Create New ApplicationType</a>
        </div>
    </div>
    @foreach(var applicationType in Model)
    {
        <div>@applicationType.Name</div>
    }
</div>